{
  "name": "umazing-musumengine",
  "version": "1.0.4",
  "private": true,
  "type": "module",
  "description": "Umamusume tools rewritten in TypeScript: decrypt/encrypt (AES-256-CBC, msgpack)",
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "umazing": "dist/cli.bundle.js"
  },
  "homepage": "https://github.com/davinidae/umazing-musumengine#readme",
  "bugs": {
    "url": "https://github.com/davinidae/umazing-musumengine/issues"
  },
  "repository": {
    "url": "https://github.com/davinidae/umazing-musumengine"
  },
  "keywords": [
    "Uma",
    "Musume",
    "UmaMusume",
    "decrypt",
    "encrypt"
  ],
  "license": "MIT",
  "funding": "https://github.com/davinidae/umazing-musumengine",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "src/cli.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json --noEmit && npm run bundle",
    "bundle": "esbuild src/cli.ts --bundle --platform=node --target=node18 --format=esm --outfile=dist/cli.bundle.js --sourcemap --external:@msgpack/msgpack --external:commander --external:fast-glob --external:fs-extra --external:node:* --tsconfig=tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.build.json --noEmit && tsc -p tsconfig.json --noEmit",
    "cli": "tsx src/cli/index.ts",
    "seed:example": "tsx scripts/seed-example.ts",
    "test": "vitest run",
    "test:unit": "vitest run --config vite.config.unit.ts",
    "test:integration": "vitest run --config vite.config.integration.ts",
    "test:coverage": "vitest run --coverage",
    "test:unit:coverage": "vitest run --config vite.config.unit.ts --coverage",
    "test:integration:coverage": "vitest run --config vite.config.integration.ts --coverage",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "precommit": "npm run lint && npm run format && npm run typecheck",
    "docs:code": "typedoc --options typedoc.code.json",
    "docs:scripts": "typedoc --options typedoc.scripts.json",
    "docs:normalize": "tsx scripts/normalize-docs.ts",
    "docs": "npm run docs:code && npm run docs:scripts && npm run docs:normalize && tsx scripts/generate-docs.ts",
    "generate:umatypes": "tsx scripts/generate-umatypes.ts && npm run format",
    "api": "tsx src/api/index.ts",
    "api:dev": "nodemon --exec tsx src/api/index.ts"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0",
    "axios": "^1.7.7",
    "commander": "^12.1.0",
    "express": "^4.19.2",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitest/coverage-v8": "^4.0.15",
    "esbuild": "^0.27.1",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typedoc": "^0.28.0",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.6.3",
    "vite": "^7.2.7",
    "vitest": "^4.0.15"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
