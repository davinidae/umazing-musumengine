{
  "name": "umazing-musumengine",
  "version": "2.0.1",
  "type": "module",
  "description": "Umamusume tools rewritten in TypeScript: decrypt/encrypt (AES-256-CBC, msgpack)",
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "umazing": "dist/cli.js"
  },
  "homepage": "https://github.com/davinidae/umazing-musumengine#readme",
  "bugs": {
    "url": "https://github.com/davinidae/umazing-musumengine/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davinidae/umazing-musumengine"
  },
  "keywords": [
    "Uma",
    "Musume",
    "UmaMusume",
    "decrypt",
    "encrypt"
  ],
  "license": "MIT",
  "funding": "https://github.com/davinidae/umazing-musumengine",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "scripts": {
    "build:js": "node scripts/build.mjs",
    "build:types": "tsc -p tsconfig.build.types.json",
    "build": "npm run build:js && npm run build:types",
    "typecheck": "tsc -p tsconfig.cli.json --noEmit && tsc -p tsconfig.json --noEmit",
    "cli": "tsx src/cli/index.ts",
    "seed:example": "tsx scripts/seed-example.ts",
    "test": "vitest run",
    "test:unit": "vitest run --config vite.config.unit.ts",
    "test:integration": "vitest run --config vite.config.integration.ts",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "prepare": "node -e \"if (require('fs').existsSync('.git')) require('child_process').execSync('husky install',{stdio:'inherit'})\"",
    "check": "npm run lint && npm run format && npm run typecheck",
    "precommit": "npm run lint && npm run format && npm run typecheck",
    "prepack": "npm run build",
    "docs:code": "typedoc --options typedoc.code.json",
    "docs:scripts": "typedoc --options typedoc.scripts.json",
    "docs:normalize": "tsx scripts/normalize-docs.ts",
    "docs": "npm run docs:code && npm run docs:scripts && npm run docs:normalize && tsx scripts/generate-docs.ts",
    "generate:umatypes": "tsx scripts/generate-umatypes.ts && npm run format"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0",
    "axios": "^1.7.7",
    "commander": "^12.1.0",
    "es-toolkit": "^1.43.0",
    "express": "^4.22.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "steamworks.js": "^0.4.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.129",
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitest/coverage-v8": "^4.0.15",
    "esbuild": "^0.27.1",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "globals": "^15.14.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.10",
    "prettier": "^3.3.3",
    "serverless": "^3.39.0",
    "serverless-esbuild": "^1.50.0",
    "serverless-offline": "^13.7.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typedoc": "^0.28.0",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.6.3",
    "vite": "^7.2.7",
    "vitest": "^4.0.15"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
